{{#if panelControls}}
    <div class="panel">
        <div class="tabcontrols">
            <div class="tabs" role="tablist" draggable="true" aria-orientation="horizontal">
            </div>
            <div class="controls">
                <div class="icon gadgetlist" role="button" aria-haspopup="true" aria-expanded="false" tabindex="0"
                     title="{{items.GADGETS-ON-PANEL}}">
                     <i class="material-icons md-light">expand_more</i></div>
                <div class="icon gadgetmover" role="button" aria-haspopup="true" aria-expanded="false" tabindex="0"
                     title="{{items.GADGETS-ACTIONS}}">
                     <i class="material-icons md-light">more_vert</i></div>
            </div>
        </div>
        <div class="content"></div>
        <div class="dropIndicator"></div>
    </div>
{{/if}}

{{#if gadgetTab}}
    <div class="tab zaz-effect zaz-effect-bar-bottom" draggable="true" tabindex="0" role="tab" aria-selected="false">
        <div class="tab-start"></div>
        <div class="tab-pin" tabindex="0"><i class="material-icons md-light">lock_open</i></div>
        <div class="tab-icon"><i class="material-icons md-light">home</i></div>
        <div class="tab-title" tabindex="0"></div>
        <div class="tab-count">0</div>
        <div class="tab-close" tabindex="0" title="{{items.CLOSE}}"><i class="material-icons md-light">close</i></div>
        <div class="tab-end"></div>
    </div>
{{/if}}

{{#if gadgetOpener}}
    <div class="zaz-overlay gadget-opener-overlay" role="dialog" tabindex="0">
        <div class="zaz-overlay-header">
            <div class="left">
                <div class="zaz-overlay-header-title" tabindex="0" aria-label="Gadgets">
                </div>
            </div>
            <div class="right">
                <i class="material-icons md-light zaz-overlay-header-close" tabindex="0" role="button" title="Close">close</i>
            </div>
        </div>
        <div class="zaz-overlay-content">
            <div class="gadget-opener-section">
                <div class="gadget-opener-title" data-mode="open">
                    <span class="open">{{items.OPEN-IN}}</span>
                    <span class="move">{{items.MOVE-TO}}</span>
                </div>
                <div class="gadget-new">
                    <div class="zaz-effect zaz-effect-bar-bottom item gadget-new-tab" data-zone="new" data-zonetype="tab" tabindex="0">New Tab</div>
                    <div class="zaz-effect zaz-effect-bar-bottom item gadget-new-window" data-zone="new" data-zonetype="window" tabindex="0">New Window</div>
                </div>
            </div>
            <div class="area-current-windows"></div>
        </div>
        <div class="zaz-overlay-footer">
            <div class="left"></div>
            <div class="right">
                <button class="button zaz-effect zaz-effect-bar-bottom default" data-action="close">Close</button>
            </div>
        </div>
    </div>
{{/if}}

{{#if gadgetMoverMenu}}
    <div class="zaz-menu gadgetmovermenu" aria-hidden="false">
        <div class="items">
            <div class="item closetab" tabindex="0" data-action="closetab">{{items.CLOSE-TAB}}</div>
            <div class="item closealltabs" tabindex="0" data-action="closealltabs">{{items.CLOSE-ALL-TABS}}</div>
            <div class="item closeothertabs" tabindex="0" data-action="closeothertabs">{{items.CLOSE-OTHER-TABS}}</div>
            <div class="item movetab" tabindex="0" data-action="movetab" style="display: none;">{{items.MOVE-TO-LOCATION}}...</div>
            <div class="item movealltabs" tabindex="0" data-action="movealltabs" style="display: none;">{{items.MOVE-ALL-TABS-TO}}...</div>
        </div>
    </div>
{{/if}}

{{#if gadgetListMenu}}
    <div class="zaz-menu gadgetlistmenu" aria-hidden="false">
        <div class="items">
        </div>
    </div>
{{/if}}

{{#if gadgetsOverlay}}
    <div class="zaz-overlay gadgets-overlay" role="dialog" tabindex="0">
        <div class="zaz-overlay-header">
            <div class="left">
                <div class="zaz-overlay-header-title" tabindex="0" aria-label="Gadgets">
                    Open Gadget(s)</div>
            </div>
            <div class="right">
                <i class="material-icons md-light zaz-overlay-header-close" tabindex="0" role="button" title="Close">close</i>
            </div>
        </div>
        <div class="zaz-overlay-content">
            <div class="gadgets"></div>
        </div>
        <div class="zaz-overlay-footer">
            <div class="left"></div>
            <div class="right">
                <button class="button zaz-effect zaz-effect-bar-bottom default" data-action="close">Close</button>
            </div>
        </div>
    </div>
{{/if}}

{{#if gadgetsOverlayItem}}
    <div class="item zaz-effect zaz-effect-bar-left-top-to-bottom" tabindex="0" aria-label="open gadget {{title}}" data-gadget="{{script}}" data-gadgetid="{{gadgetid}}">
        <i class="material-icons md-light icon-class {{iconClass}}">{{icon}}</i>
        <div class="item-meta">
            <div class="item-title">{{title}}</div>
            <div class="item-desc">{{desc}}</div>
        </div>
    </div>
{{/if}}

{{#if windowLayout}}
    <div class="area current">
        <div class="area-container">
            <div class="__layout" aria-describedby="{{idForLabel}}" data-window="{{windowName}}">
                <div class="zone" data-zone="main">
                </div>
            </div>
        </div>
        <div id="{{idForLabel}}">{{#if thisWindow}}{{items.THIS-WINDOW}} ({{items.WINDOW}} {{index}}){{else}}{{items.WINDOW}} {{index}}{{/if}}</div>
    </div>
{{/if}}