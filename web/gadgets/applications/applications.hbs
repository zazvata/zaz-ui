<div class="container zaz-effect zaz-effect-bar-top">
    <div class="gadget-controls">
        <div class="controls-bar">
            <div class="filters">
                <div tabindex="0" class="filter {{#ifequal filter 'submitted'}}active{{/ifequal}}" data-filter="submitted">Submitted<span class="count">0</span></div>
                <div tabindex="0" class="filter {{#ifequal filter 'reviewed'}}active{{/ifequal}}" data-filter="reviewed">Reviewed<span class="count">0</span></div>
                <div tabindex="0" class="filter {{#ifequal filter 'approved'}}active{{/ifequal}}" data-filter="approved">Approved<span class="count">0</span></div>
                <div tabindex="0" class="filter {{#ifequal filter 'all'}}active{{/ifequal}}" data-filter="all">All<span class="count">0</span></div>
            </div>
            <div class="knobs">
                <div tabindex="0" class="knob new-app" data-action="new-app"><label class="label">New</label></div>
                <div tabindex="0" class="knob delete-apps" data-action="delete-apps"><label class="label">Delete All</label></div>
            </div>
        </div>
    </div>
    <div class="grid"></div>
</div>